{
    "name": "win-messenger",
    "version": "0.1.4",
    "description": "A typed wrapper to window postmessage",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist/**"
    ],
    "type": "module",
    "scripts": {
        "dev": "rslib build --watch",
        "build": "rslib build",
        "test": "echo \"Error: no test specified\" && exit 1",
        "prepublishOnly": "pnpm run build",
        "format": "prettier --write .",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "release": "quick-publish"
    },
    "keywords": [
        "win-messenger",
        "window-messenger",
        "window",
        "post",
        "postmessage",
        "iframe",
        "typed",
        "window.postMessage"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/marcosvnmelo/win-messenger"
    },
    "author": "marcosvnmelo",
    "license": "MIT",
    "devDependencies": {
        "@eslint/js": "10.0.1",
        "@rslib/core": "0.19.6",
        "eslint": "10.0.2",
        "prettier": "3.8.1",
        "quick-publish": "0.7.1",
        "typescript": "5.9.3",
        "typescript-eslint": "8.56.1"
    }
}
